<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="StrictFooBarProperty">
    </transfer>

    <transfer name="StrictFooBar">
        <property name="propScalar" type="int"/>
        <property name="propScalarStrict" type="int" strict="true"/>
        <property name="propDecimal" type="decimal"/>
        <property name="propDecimalStrict" type="decimal" strict="true"/>
        <property name="propArray" type="array"/>
        <property name="propArrayStrict" type="array" strict="true"/>
        <property name="propDummyTransfer" type="StrictFooBarProperty"/>
        <property name="propDummyTransferCollection" type="StrictFooBarProperty[]" strict="true"/>
        <property name="fooTypedArray" type="string[]"/>
        <property name="fooTypedArrayStrict" type="string[]" strict="true"/>
        <property name="fooTypedArrayAssoc" type="string[]" associative="true"/>
        <property name="fooTypedArrayAssocStrict" type="string[]" associative="true" strict="true"/>
    </transfer>

</transfers>
